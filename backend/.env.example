# Supabase REST base URL (e.g., https://xxxxx.supabase.co)
SUPABASE_URL=

# Supabase anon/public API key (read-only). Do NOT use service_role for the API.
SUPABASE_API_KEY=

# Supabase service role key (prefill script writes ai_outputs)
# SUPABASE_SERVICE_ROLE_KEY=

# OpenAI keys/models (prefill)
# OPENAI_API_KEY=
# GPT_MODEL=gpt-5-mini
# EMBED_MODEL=text-embedding-3-small

# Optional: limit to a single user sub (development allowlist)
# SUPABASE_ALLOW_SUB=

# Binance symbol for gap fill (default: BTCUSDT)
# BINANCE_SYMBOL=BTCUSDT

# SSE/stream tuning (optional)
# BOOTSTRAP_PAGE_LIMIT=5000
# GAP_STREAM_MAX_MINUTES=15
# GAP_STREAM_SLEEP_SECONDS=1.5
# SESSION_TTL_SECONDS=1800
# RING_BUFFER_MINUTES=30
